  <param>
    <name>Files</name>
    <key>m_files</key>
    <type>int</type>
  </param>
  <param>
    <name>Chunks</name>
    <key>b_chunks</key>
    <type>int</type>
  </param>
  <param>
    <name>Demands</name>
    <key>L_demands</key>
    <type>int</type>
  </param>
  <param>
    <name>Small Packet Length</name>
    <key>small_packet_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Length Tag Key</name>
    <key>len_tag_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <param>
    <name>Debug</name>
    <key>DEBUG</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
</block>
