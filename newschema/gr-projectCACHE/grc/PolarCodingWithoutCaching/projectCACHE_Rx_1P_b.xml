<?xml version="1.0"?>
<block>
  <name>User</name>
  <key>projectCACHE_Rx_1P_b</key>
  <category>projectCACHE</category>
  <import>import projectCACHE</import>
  <make>projectCACHE.Rx_1P_b($id_utente, $n_utenti, $m_files, $b_chunks, $L_demands, $small_packet_len, $len_tag_key, $DEBUG)</make>
  <param>
    <name>ID User</name>
    <key>id_utente</key>
    <type>int</type>
  </param>
  <param>
    <name>Users</name>
    <key>n_utenti</key>
