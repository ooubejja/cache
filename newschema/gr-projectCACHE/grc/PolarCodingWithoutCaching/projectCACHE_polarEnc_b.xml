<?xml version="1.0"?>
<block>
  <name>Polar Encoder</name>
  <key>projectCACHE_polarEnc_b</key>
  <category>[projectCACHE]</category>
  <import>import projectCACHE</import>
  <make>projectCACHE.polarEnc_b($N, $K_w, $K_s, $SNR_w, $SNR_s)</make>
  <param>
    <name>CodeWord Length</name>
    <key>N</key>
    <type>int</type>
  </param>
  <param>
    <name>Weak Msg Length</name>
    <key>K_w</key>
    <type>int</type>
  </param>
  <param>
    <name>Strong Msg Length</name>
    <key>K_s</key>
    <type>int</type>
  </param>
  <param>
    <name>SNR weak</name>
    <key>SNR_w</key>
    <type>float</type>
  </param>
  <param>
    <name>SNR Strong</name>
    <key>SNR_s</key>
    <type>float</type>
  </param>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
